<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Id>Kaomi.Client</Id>
    <Title>Kaomi.Client</Title>
    <Version>0.5.0</Version>
    <Authors>carlubian</Authors>
    <Owners>carlubian</Owners>
    <Company>carlubian</Company>
    <PackageReleaseNotes>Initial NuGet release.</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/carlubian/Kaomi</PackageProjectUrl>
    <Description>Allows the creation of Kaomi Clients.</Description>
    <RepositoryUrl>https://github.com/carlubian/Kaomi.git</RepositoryUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <PackageTags>asp-net-core, web-api, dotnet-core3</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ConfigAdapter" Version="1.2.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
  </ItemGroup>

</Project>
